{
    "env": {
        "myIncludePath": [
            "${workspaceFolder}/packages/game/include",
            "${workspaceFolder}/packages/game/include/libc"
        ],
        "myDefines": [
            "_LANGUAGE_C=1",
            "F3DEX_GBI_2 = 1",
            "DEBUG = 1"
        ]
    },
    "configurations": [
        {
            "name": "Native"
        },
        {
            "name": "OoT",
            "includePath": [
                "${myIncludePath}",
                "${workspaceFolder}/packages/core/src/oot/ovl"
            ],
            "defines": [
                "${myDefines}",
                "GAME_OOT=1"
            ],
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x86",
            "compilerPath": ""
        },
        {
            "name": "MM",
            "includePath": [
                "${myIncludePath}",
                "${workspaceFolder}/packages/core/src/mm/ovl"
            ],
            "defines": [
                "${myDefines}",
                "GAME_MM=1"
            ],
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x86",
            "compilerPath": ""
        }
    ],
    "version": 4
}
