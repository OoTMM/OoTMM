{
    "env": {
        "myIncludePath": [
            "${workspaceFolder}/third_party/ultralib/include/gcc",
            "${workspaceFolder}/third_party/ultralib/include",
            "${workspaceFolder}/packages/generator/build/include",
            "${workspaceFolder}/packages/generator/include",
            "${workspaceFolder}/packages/generator/src/common/ovl"
        ],
        "myDefines": [
            "_LANGUAGE_C=1",
            "F3DEX_GBI_2 = 1",
            "DEBUG = 1"
        ]
    },
    "configurations": [
        {
            "name": "Native"
        },
        {
            "name": "Logic",
            "configurationProvider": "ms-vscode.cmake-tools",
            "cppStandard": "c++14",
            "cStandard": "c11",
            "compileCommands": "${workspaceFolder}/packages/logic/build/compile_commands.json"
        },
        {
            "name": "OoT",
            "includePath": [
                "${myIncludePath}",
                "${workspaceFolder}/packages/generator/src/oot/ovl"
            ],
            "defines": [
                "${myDefines}",
                "GAME_OOT=1"
            ],
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x86",
            "compilerPath": ""
        },
        {
            "name": "MM",
            "includePath": [
                "${myIncludePath}",
                "${workspaceFolder}/packages/generator/src/mm/ovl"
            ],
            "defines": [
                "${myDefines}",
                "GAME_MM=1"
            ],
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x86",
            "compilerPath": ""
        }
    ],
    "version": 4
}
